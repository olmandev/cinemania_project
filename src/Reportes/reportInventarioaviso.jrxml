	<queryString language="SQL">
		<![CDATA[SELECT p.id,
       p.descripcion,
       p.stockMin,
       p.stockMax,
       COALESCE((
         SELECT SUM(CASE WHEN mi.movimiento='entrada' THEN mi.cantidad ELSE 0 END)
              - SUM(CASE WHEN mi.movimiento='salida'  THEN mi.cantidad ELSE 0 END)
         FROM movimientos_inventario mi
         WHERE mi.descripcion = p.descripcion
       ),0) AS existencia,
       CASE
         WHEN COALESCE((
           SELECT SUM(CASE WHEN mi2.movimiento='entrada' THEN mi2.cantidad ELSE 0 END)
                - SUM(CASE WHEN mi2.movimiento='salida'  THEN mi2.cantidad ELSE 0 END)
           FROM movimientos_inventario mi2
           WHERE mi2.descripcion = p.descripcion
         ),0) <= p.stockMin THEN 'ROJO (Reponer ya)'
         WHEN COALESCE((
           SELECT SUM(CASE WHEN mi3.movimiento='entrada' THEN mi3.cantidad ELSE 0 END)
                - SUM(CASE WHEN mi3.movimiento='salida'  THEN mi3.cantidad ELSE 0 END)
           FROM movimientos_inventario mi3
           WHERE mi3.descripcion = p.descripcion
         ),0) <= p.stockMin + CEIL(0.2 * NULLIF(p.stockMax,0)) THEN 'AMARILLO'
         ELSE 'VERDE'
       END AS alerta
FROM productos p
ORDER BY alerta, p.descripcion;
]]>
	</queryString>
