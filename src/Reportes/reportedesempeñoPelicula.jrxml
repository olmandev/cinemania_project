	<queryString language="SQL">
		<![CDATA[SELECT v.pelicula,
       SUM(CASE WHEN d.nombre_producto LIKE 'BOLETO%' THEN d.cantidad ELSE 0 END)                           AS boletos,
       SUM(CASE WHEN d.nombre_producto LIKE 'BOLETO%' THEN d.cantidad * d.precio_unitario ELSE 0 END)       AS ingreso_taquilla,
       SUM(d.cantidad * d.precio_unitario)                                                                  AS ingreso_total
FROM ventas v
LEFT JOIN detalle d ON d.id_venta = v.id_venta
WHERE v.estado = 'Activo'
  -- AND v.fecha BETWEEN :P_FECHA_INI AND :P_FECHA_FIN
GROUP BY v.pelicula
ORDER BY ingreso_total DESC;
]]>
	</queryString>
