	<queryString>
		<![CDATA[SELECT
     ventas.`id_venta` AS ventas_id_venta,
     ventas.`cliente` AS ventas_cliente,
     ventas.`pelicula` AS ventas_pelicula,
     ventas.`sala` AS ventas_sala,
     ventas.`asiento` AS ventas_asiento,
     ventas.`subtotal` AS ventas_subtotal,
     ventas.`isv` AS ventas_isv,
     ventas.`total` AS ventas_total,
     ventas.`metodo_pago` AS ventas_metodo_pago,
     ventas.`cantidad_pagar` AS ventas_cantidad_pagar,
     ventas.`cambio` AS ventas_cambio,
     ventas.`fecha` AS ventas_fecha,
     ventas.`estado` AS ventas_estado
FROM
     `ventas` ventas
WHERE ventas.fecha IS NOT NULL
  AND ventas.fecha <> '0000-00-00'
  AND ventas.fecha BETWEEN $P{Fechai} AND $P{Fechaf}]]>
	</queryString>
